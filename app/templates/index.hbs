<div class="well">
  <div class="row">
    <div class="col-sm-6 col-sm-offset-1">
      <h3 class="index-label">Question:</h3>
    </div>
    <div class="col-sm-4 col-sm-offset-1">
      <h3 class="index-label">Asked by:</h3>
    </div>
  </div>

  <div class="row">
    {{#each model.questions as |question|}}
      {{question-tile question=question}}
    {{/each}}
  </div>
</div>

{{add-question saveQuestion="saveQuestion"}}
